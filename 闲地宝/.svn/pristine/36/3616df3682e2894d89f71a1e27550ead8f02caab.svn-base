    <title>收益明细</title>
    <style type="text/css">
		html,body{
			background: background: #F5F5F5;;
		}
		.sel_wrap{
			height: 1.36rem;
			padding-left: 0.426666rem;
		}
		.plus{
			width: 1.973333rem;
			height: 0.8rem;
			border-radius: 0.4rem;
			background: rgba(177,177,177,0.12);
			line-height: 0.8rem;
			text-align: center;
			font-size: 0.4rem;
			color: #2D3134;
			
		}
		.plus .sel{
			margin-left: 0.133333rem;
			display: inline-block;
			width: 0.346666rem ;
			height: 0.186666rem;
			background: url(../../icon/sel.png) no-repeat;
			background-size: 0.346666rem 0.186666rem;
		}
		.mui-popover.mui-popover-action .mui-table-view{
			margin: 0;
			border-radius: 0;
		}
		.mui-table-view-cell{
			padding: 0;
			height: 1.493333rem;
			line-height: 1.493333rem;
			color: #282828;
			font-size: 0.48rem;
		}
		.mui-table-view-cell > a:not(.mui-btn){
			margin: 0;
		}
		.earing_wrap{
			background: #fff;
		}
		.earing_wrap li{
			height: 1.733333rem;
			box-sizing: border-box;
			overflow: hidden;
			padding-left:0.426666rem;
		}
		.earing_wrap li .icon{
			width: 0.933333rem;
			height: 0.933333rem;
			box-sizing: border-box;
			overflow: hidden;
		}
		.earing_wrap li .icon img{
			width: 100%;
			height: 100%;
		}
		.earing_wrap li .info{
			flex: 1;
			padding-left:0.266666rem ;
			padding-right: 0.426666rem;
			position: relative;
		}
		.earing_wrap li .info:after{
			content: '';
			height: 1px;
			background: rgba(0,0,0,0.1);
			position: absolute;
			bottom: 0;
			left: 0;
			right: 0;
			transform: scaleY(.5);
		}
		.earing_wrap li .title{
			font-size: 0.346666rem;
			color: #2D3134;
			margin-bottom: 0.266666rem;
		}
		.earing_wrap li .time{
			font-size: 0.266666rem;
			color: #B1B1B1;
		}
		.earing_wrap li .info .right{
			font-size: 0.533333rem;
			color: #0097FE;
		}
		.earing_wrap li .info .right.cut{
			color: #B1B1B1;
		}
		.earing_wrap li .info .right.cut span{
			display: inline-block;
			width: 0.266666rem;
			height: 0.053333rem;
			background: url(../../icon/cut.png) no-repeat;
			background-size: 0.266666rem 0.053333rem;
			margin-right: 0.08rem;
			vertical-align: 0.133333rem;
		}
		.earing_wrap li .info .right span{
			display: inline-block;
			width: 0.266666rem;
			height: 0.266666rem;
			background: url(../../icon/add_2.png) no-repeat;
			background-size: 0.266666rem 0.266666rem;
			margin-right: 0.08rem;
			vertical-align: 1px;
		}
    </style>
</head>

<body>
