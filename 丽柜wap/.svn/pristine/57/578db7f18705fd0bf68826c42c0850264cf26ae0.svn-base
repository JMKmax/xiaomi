<script>
    apiready = function () {
        quakooData.ajaxGetData(config.getUrl_helpCenter_getDetail(),{type:2},function(ret,err){
        	if(ret && ret.success){
        		var data = {
        			list:ret.data
        		}
        		$api.html($api.dom('.contents-list'),ret.data.answer)
        	}
        	
        })
    }
</script>

