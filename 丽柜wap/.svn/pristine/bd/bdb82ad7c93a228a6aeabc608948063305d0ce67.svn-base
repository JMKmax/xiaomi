<!DOCTYPE html>
<html>

	<head>
		<meta charset="UTF-8">
		<meta name="viewport" content="width=device-width, 		initial-scale=1,maximum-scale=1,user-scalable=no">
		<meta name="apple-mobile-web-app-capable" content="yes">
		<meta name="apple-mobile-web-app-status-bar-style" content="black">
		<meta name="format-detection" content="telephone=no" />
		<link rel="stylesheet" type="text/css" href="../../css/api.css" />
		<link rel="stylesheet" type="text/css" href="../../css/commonWindow.css" />
		<title>排行榜</title>
		<style>
			.second-nav {
				background:#f5f5f5;
				height:68px;
				display: flex;
				align-items: center;
				justify-content: center;
			}
			.second-nav ul{
				display: flex;
				width:134px;
				height:26px;
				box-sizing: border-box;
				background: #fff;
				border:1px solid #ff84b2;
				border-radius:100px;
				overflow: hidden;
			}
			.second-nav ul li{
				flex: 1;
				display: flex;
				align-items: center;
				justify-content: center;
				font-size:14px;
				color:#ff5c98;
			}
			.second-nav ul li.active {
				background:#ff5c98;
				color: #fff;
			}
		</style>
	</head>

	<body>
		
	</body>
	<script type="text/javascript" src="../../lib/script/quakooLib/QuakooBase-1.0.0.js"></script>
	<script type="text/javascript" src="../../lib/script/quakooLib/QuakooConfig-1.0.0.js"></script>
	<script type="text/javascript" src="../../lib/script/quakooLib/QuakooBaseApp-1.0.0.js"></script>
	<script type="text/javascript" src="../../lib/script/quakooLib/QuakooBaseAppBusiness-1.0.0.js"></script>
	<script type="text/javascript" src="../../lib/script/quakoo/Config.js"></script>

	<script type="text/javascript" src="../../script/api.js"></script>
	<script type="text/javascript" src="../../lib/script/quakoo/project.js"></script>
	<script>
		var params;
		apiready = function() {
			params = api.pageParam;
			var statusBarAppearance = api.statusBarAppearance; //是否支持沉浸式
			var header = document.querySelector('#topbar');
			if(statusBarAppearance) {
				$api.fixStatusBar(header);
			}
			api.openFrameGroup({
				name: 'rankGroup',
				background: "#f5f5f5",
				scrollEnabled: true,
				rect: {
					x: 0,
					marginTop: 44 + 68 + api.safeArea.top,
					w: "auto",
					h: 'auto'
				},
				index: 0,
				frames: [{
						name: 'rank_gongXian',
						bgColor: "#F5F5F5",
						pageParam: {},
						url: './rank_gongXian.html',
						bounces:false,
					},
					{
						name: 'rank_earnings',
						bgColor: "#F5F5F5",
						pageParam: {},
						url: './rank_earnings.html',
						bounces:false,
					}
				]
			}, function(ret, err) {
				changeStyle(ret.index)
			})
		};

		function changeStyle(index) {
			var labs = $api.domAll('.second-nav ul li');
			labs.forEach(function(item, i) {
				if(index == i) {
					$api.addCls(item, 'active')
				} else {
					$api.removeCls(item, 'active')
				}
			})
		}

		function groupFrameSwitch(index) {
			changeStyle(index);
			api.setFrameGroupIndex({
				name: "rankGroup",
				index: index,
				scroll:true
			});
		}

	</script>

</html>