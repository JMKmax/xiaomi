        var ajaxParams ={
            type:1,
            objectId:prodId,
            content:content
        };
        quakooData.ajaxSubmitDataNotProcess(config.getUrl_comment_add(), ajaxParams, function (ret, err) {
            if (ret && ret.success) {
                api.execScript({
                    name:api.winName,
