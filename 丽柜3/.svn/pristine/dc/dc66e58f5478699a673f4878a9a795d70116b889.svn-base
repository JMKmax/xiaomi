            //é¡¶éƒ¨ å›¾ç‰‡åˆ—è¡¨ over
            var info = quakooUtils.tryDecodeURIComponent(results.info);
            results.info = info;

            results.ctime = quakooUtils.formatTimeToDateDianLY(results.ctime);

            // æ¨¡ç‰¹å¤´åƒ
            if (quakooUtils.isBlack(results.modelIcon)) {
